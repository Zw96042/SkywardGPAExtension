{
  "manifest_version": 3,
  "name": "Skyward GPA Calculator",
  "version": "2.0",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/scripts/wsisa.dll/WService=ws*/*",
    "https://*/scripts/wsisa.dll/WService=ws*/*"
  ],
  "description": "Calculate your GPA easily! Log into Skyward & navigate to your gradebook.",
  "icons": {
    "512": "icon.png",
    "32": "icon32.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "up_/css/variables.css",
        "up_/css/qsfmain001.css",
        "up_/css/sfhome001.css",
        "up_/css/jquery.cluetip.css",
        "up_/css/sfgradebook001.css",
        "up_/css/qclssbase001.css",
        "up_/css/qclsslogin001.css",
        "arrows_sprite.7956fbd8.png",
        "printer1.35c0747c.png",
        "sf_bg_sprite001.b3d934ac.png",
        "sf_icons_sprite.e462d1c9.png",
        "up_/assets/images/SkyLogoBlue.png",
        "gpachart/gpachart.html",
        "script2.js"
      ],
      "matches": [
        "https://skyward.iscorp.com/*",
        "http://skyward.iscorp.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
          "http://*/scripts/wsisa.dll/WService=*",
          "https://*/scripts/wsisa.dll/WService=*"
        ],
      "js": ["content.js"]
    }
  ]
}
